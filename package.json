{
  "name": "react-native-apply-version",
  "version": "1.1.1",
  "description": "Apply the version in package.json to android/app/build.gradle and info.plist",
  "license": "MIT",
  "bin": {
    "applyVersion": "./build/index.js",
    "apply-version": "./build/index.js"
  },
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sidevesh/react-native-apply-version"
  },
  "keywords": [
    "version",
    "applyVersion",
    "apply-version",
    "node",
    "javascript",
    "react-native",
    "react",
    "semver"
  ],
  "bugs": {
    "url": "https://github.com/sidevesh/react-native-apply-version/issues"
  },
  "homepage": "https://github.com/sidevesh/react-native-apply-version#readme",
  "main": "index.js",
  "author": "Novom Interactive",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "babel src --out-dir build --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\"",
    "lint": "eslint .",
    "start": "node .src/index.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "plist": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
